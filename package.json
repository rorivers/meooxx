{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.4.1",
  "description": "A boilerplate for React-Babel-Webpack project",
  "main": "app/main.jsx",
  "scripts": {
    "precommit": "npm run lint && prettier --no-config --write ./app/components/*",
    "lint": "eslint --fix --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "test": "npm run lint",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "start": "webpack",
    "start:server": "webpack-dev-server",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "antd": "^3.21.2",
    "autoprefix": "^1.0.1",
    "babel-plugin-import": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "isomorphic-fetch": "^2.2.1",
    "markdown": "^0.5.0",
    "postcss": "^7.0.17",
    "postcss-autoreset": "^2.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "postcss-smart-import": "^0.6.10",
    "precss": "^4.0.0",
    "prettier": "^1.10.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "url-loader": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "^2.0.23",
    "copy-webpack-plugin": "latest",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "latest",
    "husky": "^3.0.2",
    "jquery": "^3.4.1",
    "npm-check": "^5.9.0",
    "open-browser-webpack-plugin": "0.0.3",
    "style-loader": "~0.13.1",
    "validate-commit-msg": "2.x",
    "webpack": "latest",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "latest"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "superq",
  "license": "MIT"
}
